    <h4 class="text-muted">Time Left: <span id="time"><span></h4>
  {{/if}}
</div>

<div class="col-md-8">
  <form method="post" action"/studentcourse/viewTest">
    <div class="questions">

    </div>
    <div class="">
      <button class="btn btn-primary btn-lg pull-right" name="button" type="submit">Submit</button>
    </div>
  </form>
</div>

{{#section 'script'}}
  <script src="/js/testQuestions.js"></script>
{{/section}}

{{#section 'timer'}}
  <script src="/js/timer.js"></script>
{{/section}}
