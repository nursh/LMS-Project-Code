<div class="row">
  <div class="tests col-md-6">
    <h2> {{cname}} &mdash; Tests</h2>
    {{#if ctests }}
      <p class="alert alert-info"><span class="glyphicon glyphicon-info-sign" aria-hidden="true"></span>  {{ ctests }} </p>
    {{/if}}
    {{#if tans }}
      <p class="alert alert-success"><span class="glyphicon glyphicon-info-sign" aria-hidden="true"></span>  {{ tans }} </p>
    {{/if}}
    {{#if afail }}
      <p class="alert alert-warning"><span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>  {{ afail }} </p>
    {{/if}}
  </div>
</div>


{{#section 'script'}}
  <script src="/js/studentCourseTests.js"></script>
{{/section}}
